@using NW.Income.UIServices.Extensions
@model IEnumerable<NW.Income.UIServices.Controllers.Questionaire>

@{
    ViewBag.Title = "DynamicContentApplication";
}

<h2>DynamicContentApplication</h2>
<h2>@ViewData["applicationId"] </h2>




<p>
    @* @Html.InlineStyles(@ViewData["applicationId"] as string)*@
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Question)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Question)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
@section Styles {
    @* <link href="~/Content/blueTheme.css" rel="stylesheet" />*@
    @Html.InlineStyles(@ViewData["applicationId"] as string)
}